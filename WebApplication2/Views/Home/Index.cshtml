<!-- Start Team -->
<section class="team-sec" id="team-sec">
    <div class="container">
        <!--Heading-->
        <div class="row text-center">
            <div class="col-12 col-md-10 col-lg-8 offset-md-1 offset-lg-2 wow zoomIn heading-area" data-wow-duration="1s" data-wow-delay=".1s">
                <h3 class="heading text-center"><span class="text-red d-block js-rotating">Creativity,Innovation,Inspiration</span> in our blood</h3>
                <p class="text text-center">Lorem ipsum is simply dummy text of the printing and typesetting. Lorem Ipsum has been the industry’s standard dummy.</p>
            </div>
        </div>

        <div class="team owl-carousel owl-theme text-center padding-top-half">
            <!-- Item-1 -->
            @foreach (var team in Model.Teams)
           {
               
            <div class="item team-info">
                <div class="image-holder">
                    <img src="~/bubble-studio/img/@team.Image" alt="Team">
                    <div class="overlay d-flex justify-content-center align-items-center">
                        <div class="speaker-links">
                            <ul>
                                @foreach(var social in team.TeamSocials)
                                {
                                    <li>
                                        <a href="@social.Url"><i class="@social.Icon"></i></a>
                                    </li>
                                }
                            </ul>
                        </div>
                    </div>
                </div>
                <h4 class="speaker-name">@team.Name</h4>
                <p class="speaker-detail">@team.Image</p>
            </div>
           }
        </div>

        <div class="col-12 padding-top-half">
            <ul class="services-boxes">
                <!-- Service-1 -->
                @foreach (var slogan in Model.Slogans)
                {
                    count++;
                    if (count%2==1)
                    {
                        <li class="service-card left wow slideInLeft">
                            <div class="icon-holder"><i class="@slogan.Icon"></i></div>
                            <p class="text">@slogan.Text</p>
                        </li>
                    }
                    else
                    {
                    <!-- Service-2 -->
                <li class="service-card right d-block text-center wow slideInRight">
                    <div class="icon-holder"><i class="@slogan.Icon"></i></div>
                    <p class="text">@slogan.Text</p>
                </li>
                    }
                }
            </ul>
        </div>
    </div>
</section>
<!-- End Team -->
