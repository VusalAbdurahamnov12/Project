<!-- Start Contact -->
<section id="contact">
    <div class="container">
        <!--Heading-->
        <div class="row text-center wow zoomIn" data-wow-duration="1s" data-wow-delay=".3s">
            <div class="col-12 col-md-10 col-lg-8 offset-md-1 offset-lg-2 heading-area">
                <h3 class="heading text-center">Let’s Get Connected</h3>
            </div>
        </div>
        <!--Social-->
        <div class="row social-links wow zoomIn" data-wow-duration="1s" data-wow-delay=".6s">
            @foreach (var setting in Model.Settings)
           {
               
            <div class="col-12 col-md-12 col-lg-4 d-flex justify-content-center">
                <a><i aria-hidden="true" class="@setting.Key"></i>@setting.Value</a>
            </div>
           }
        </div>


    

        <!--contact us-->
        <form class="contact-form" asp-action="contact" id="contact-form-data" method="post">
            <div class="row wow fadeIn" data-wow-duration="1.5s" data-wow-delay=".9s">

                <div class="col-12 col-md-10 col-lg-10 offset-md-1 offset-lg-1" id="result">
                    @if(TempData["successfull"] != null)
                    {
                        <div class="alert alert-success" role="alert">
                            @TempData["successfull"]
                        </div>
                    }
                </div>

                <div class="col-12 col-md-10 col-lg-10 offset-md-1 offset-lg-1">
                    <div class="form-group">
                        <input class="form-control" asp-for="Contact.Name" placeholder="Name:" id="first_name">
                        <span class="text-danger" asp-validation-for="Contact.Name"></span>
                    </div>
                    <div class="form-group">
                        <input class="form-control" type="tel" asp-for="Contact.ContactNumber" placeholder="Contact No:" id="phone">
                        <span class="text-danger" asp-validation-for="Contact.ContactNumber"></span>
                    </div>
                    <div class="form-group">
                        <input class="form-control" asp-for="Contact.Email" placeholder="Email:" id="email">
                        <span class="text-danger" asp-validation-for="Contact.Email"></span>
                    </div>
                    <div class="form-group">
                        <textarea class="form-control" asp-for="Contact.Message" placeholder="Type Your Message Here" id="message"></textarea>
                        <span class="text-danger" asp-validation-for="Contact.Message"></span>
                    </div>
                    <button type="submit" class="btn contact_btn btn-large btn-blue btn-hvr-blue btn-rounded mt-4 w-100" id="submit_btn">
                        SUBMIT INFORMATION
                        <div class="btn-hvr-setting">
                            <ul class="btn-hvr-setting-inner">
                                <li class="btn-hvr-effect"></li>
                                <li class="btn-hvr-effect"></li>
                                <li class="btn-hvr-effect"></li>
                                <li class="btn-hvr-effect"></li>
                            </ul>
                        </div>
                    </button>
                </div>
            </div>
        </form>
    </div>
</section>
<!-- End Contact -->
