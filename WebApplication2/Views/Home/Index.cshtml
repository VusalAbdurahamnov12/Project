<!-- Start Review -->
<section class="testimonial" id="review">
    <div class="container">
        <div class="row pt-3">
            <div class="col-12">
                <!-- Testimonial Slider -->
                <div id="testimonial-carousal" class="testimonial-owl owl-carousel owl-theme">
                    @foreach (var Comment in Model.Comments)
                    {
                            
                    <!-- Item-1 -->
                    <div class="item text-center">
                        <div class="description">
                            <p class="paragraph">@Comment.Message</p>
                        </div>
                        <div class="testimonial-img">
                            <img src="~/bubble-studio/img/@Comment.Image" alt="">
                        </div>
                        <div class="testimonial-tittle mt-3 mb-3">
                            <h4 class="text-red main-font">@Comment.Name</h4>
                            <span class="alt-font">@Comment.WorkingPlace</span>
                        </div>
                    </div>
                    }
                </div>
            </div>
        </div>
    </div>
</section>
<!-- End Review -->
<!-- Start Portfolio -->
<section class="portfolio-sec" id="portfolio-sec">
    <div class="container">
        <div class="row portfolio-heading-area text-center text-lg-left">
            <div class="col-12 col-md-10 offset-md-1 col-lg-6 offset-lg-0">
                <h4 class="heading wow fadeInLeft">Our recent work & <span class="text-red">projects</span></h4>
            </div>
            <div class="col-12 col-md-10 offset-md-1 col-lg-5 offset-lg-1">
                <p class="text wow fadeInRight pt-3">Lorem ipsum is simply dummy text of the printing and typesetting. Lorem Ipsum has been the industry’s standard dummy.  Lorem Ipsum has been the industry’s standard dummy.</p>
            </div>
        </div>
        <div class="row position-relative">
            <div class="col-12 port-image">
                <div class="portfolio-carousel owl-carousel owl-item">
                    <!-- Item -->
                    @foreach (var slider in Model.Sliders)
                   {
                       
                    <div class="item">
                        <a href="/bubble-studio/standalone.html" class="portfolio-image">

                            <img src="~/bubble-studio/img/@slider.Image" alt="Image">
                        </a>
                        <div class="row no-gutters portfolio-footer padding-top">
                            <div class="col-4 col-lg-4 d-flex justify-content-center justify-content-lg-end">
                                <div class="portfolio-mini-detail">
                                    <h4 class="portfolio-mini-heading">Earphones</h4>
                                    <p class="text">Client</p>
                                </div>
                            </div>
                            <div class="col-4 col-lg-4 d-flex justify-content-center">
                                <div class="portfolio-mini-detail">
                                    <h4 class="portfolio-mini-heading">Aug 2020</h4>
                                    <p class="text">Date</p>
                                </div>
                            </div>
                            <div class="col-4 col-lg-4 d-flex justify-content-center justify-content-lg-start">
                                <div class="portfolio-mini-detail">
                                    <h4 class="portfolio-mini-heading">Website Design</h4>
                                    <p class="text">Services</p>
                                </div>
                            </div>
                        </div>
                    </div>
                   }
                </div>
                <a class="portfolio-links" id="portfolio-arr-left"><i class="fas fa-angle-left"></i></a>
                <a class="portfolio-links" id="portfolio-arr-right"><i class="fas fa-angle-right"></i></a>
            </div>
        </div>
    </div>
</section>
<!-- End Portfolio -->
